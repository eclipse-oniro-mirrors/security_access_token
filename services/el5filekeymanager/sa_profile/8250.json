{
    "process": "el5_filekey_manager",
    "systemability": [
        {
            "name": 8250,
            "libpath": "libel5_filekey_manager_service.z.so",
            "run-on-create": false,
            "distributed": false,
            "dump_level": 1,
            "start-on-demand": {
                "commonevent": [
                    {
                        "name": "usual.event.SCREEN_LOCKED",
                        "conditions": [
                            {
                                "eventId": "commonevent",
                                "name": "usual.event.USER_REMOVED"
                            }
                        ]
                    },
                    {
                        "name": "usual.event.USER_REMOVED",
                        "conditions": [
                            {
                                "eventId": "commonevent",
                                "name": "usual.event.SCREEN_LOCKED"
                            }
                        ]
                    }
                ]
            }
        }
    ]
}
